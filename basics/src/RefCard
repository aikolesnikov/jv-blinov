[public] [final] [abstract] class ИмяКласса {
 {} // логические блоки

 // внутренние классы

// поля
 private // закрытые
 public // открытые
 // дружественные (по умолчанию)
 protected // защищенные

// конструкторы
 public // открытые
 // дружественные (по умолчанию)
 protected // защищенные
 private // закрытые

// методы
 public // открытые
 ‎// дружественные (по умолчанию)
 protected // защищенные
 private // закрытые
}


